{
  "name": "jenkins",
  "default_attributes": {
    "jenkins": {
      "master": {
        "install_method": "package"
      }
    },
    "java": {
      "install_flavor": "oracle",
      "jdk_version": "8",
      "oracle": {
        "accept_oracle_download_terms": true
      }
    }
  },
  "run_list": [
    "recipe[jenkins::java]",
    "recipe[jenkins::master]",
    "recipe[java]"
  ]
}
